@model IEnumerable<Wiser.Models.Wisdom.WisdomScrollItem>

<h2 style="font-family: 'Cinzel';">Your Wisdom</h2>
@if (Model.Count() == 0)
{
    <div class="jumbotron noFavorites">
        <h1 class="centered">
            Unfortunately, you currently have no favorites.
            Go to the Infinity Scroll to find some wisdom you'd like to favorite!
        </h1>
    </div>
}
@Html.Partial("ScrollItem", Model)